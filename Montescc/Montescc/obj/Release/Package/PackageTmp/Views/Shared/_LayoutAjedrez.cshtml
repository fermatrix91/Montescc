<!DOCTYPE HTML STYLE="zoom: reset;">

<html lang="es"
      oncontextmenu="return false">
<head>
    @*<title>Lecciones GRATIS de Ajedrez</title>*@
    <title>@ViewBag.TituloContenido</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta property="og:description" name="description" content="@ViewBag.DescripcionContenido" />
    <meta property="og:image" content="@ViewBag.UrlImagenPrincipal" />
    @*<meta name="description" content="Lecciones de ajedrez gratis - Miles de ejercicios para desarrollar habilidades ajedrecíscticas - Enlaces a los mejores sitios para desafiar a miles de jugadores de ajedrez en todo el mundo - Aperturas, finales y jugadas de leyendas de todos los tiempos." />*@
    <meta name="keywords" content="Ajedrez, Chess, Ajedrez Gratis, Aprende Ajedrez Gratis, Lecciones de Ajedrez Gratis" />

    <script src="~/Scripts/jquery-2.2.3.js"></script>
    <script src="~/Scripts/jquery-ui-1.8.24.js"></script>
    <link href="~/Content/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/menu/stylesMenu.css" rel="stylesheet" />
    <link href="~/Content/app.css" rel="stylesheet" />
    <script src="~/Content/menu/scriptMenu.js"></script>

    @Scripts.Render("/Scripts/bloquear.js")
</head>

<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<script>
    (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "ca-pub-4671422388806733",
        enable_page_level_ads: true
    });
</script>

<body onkeydown="checkKey(event)"
      oncontextmenu="return false">

    <div id="contenedorSitio" class="anchuraContenedor bordeSitio" style="background:white; position: absolute;">
        <header id="menuSuperior" class="anchuraContenedor navbar-fixed-top" style="border-bottom: 2px solid rgb(212, 81, 81);">
            <article>
                <div id='cssmenu' style="border-bottom: 2px solid rgb(212, 81, 81);">
                    <ul>
                        <li style="height: 39px;"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Home/Index'>Inicio</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Home/Index'>Home</a></li>
                        <li style="height: 39px;" id="cuadroInglesEspanol" style="z-index:1000;"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Ingles/?idPagina=1'>Inglés - Español</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Ingles/?idPagina=1'>English - Spanish</a></li>
                        <li style="height: 39px;" id="cuadroAjedrez"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Ajedrez'>Ajedrez</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Ajedrez'>Chess</a></li>
                        <li style="height: 39px;" id="cuadroWeb"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/AspNetMvc'>Asp.Net-Mvc</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/AspNetMvc'>Asp.Net-Mvc</a></li>
                        <li style="height: 39px;" id="cuadroContabilidad"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Contabilidad'>Contabilidad</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Contabilidad'>Accounting</a></li>
                        <li style="height: 39px;" id="cuadroNoticias"><a style="padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Noticia'>Novedades</a><a style="color: #ecd24d; padding-top: 5px;padding-bottom: 0; text-align: center;" href='/Curso/Noticia'>News</a></li>
                        <li style="height: 39px;"><button id="abreFormulario" style="padding-top: 0px;padding-bottom: 0;background-color: #333333; border: none; border-style: none;" data-toggle="modal" data-target="#myModal"><p style="font-size:12px; padding-top: 5px;padding-bottom: 0; margin: 0;">Contáctanos</p><p style="font-size:12px; padding-top: 5px;padding-bottom: 0; margin: 0;color: #ecd24d;">Contact us</p> </button></li>
                    </ul>
                </div>
            </article>
        </header>

        <section>
            <!-- oncontextmenu="return false"> -->
            <a href="#" class="scrollup">Scroll</a>
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </section>

        <!-- Modal -->
        <div id="myModal" class="modal fade" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Contáctanos</h4>
                    </div>
                    <form id="formCorreo" action="/Home/CorreoContactanos" method="post">
                        <div class="modal-body">
                            <p>Tu Correo Electrónico:</p>
                            <input placeholder="Email ej: usuario@dominio.com" id="correoContacto" name="correoContacto" type="email" class="form-control" />
                            <p>Mensaje:</p>
                            <textarea rows="3" placeholder="Contenido del mensaje" id="contenidoContacto" name="contenidoContacto" class="form-control"></textarea>
                        </div>
                        <div class="modal-footer">
                            <input id="botonContactanos" type="submit" class="btn btn-success" data-dismiss="modal" value="Enviar" />
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>

    <div class="modal fade" id="imagemodal" style="overflow: auto;" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        @*<div class="modal-dialog" style="width: auto;">
            <div class="modal-content">
                <div class="modal-body">*@
        <button type="button" class="close" data-dismiss="modal" style="margin-top: -10px; opacity: 1!important; background-color: #b7b7b7 !important; position: fixed; right: 0;"><span style="color:white !important; font-weight: bolder; font-size: 55px;" aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <img src="" class="imagepreview" style="width: auto; height:auto; display: block; margin:0 auto;">
        @*</div>
                </div>
            </div>*@
    </div>

    <script src="~/Content/bootstrap/js/bootstrap.js"></script>
    @*@Scripts.Render("~/bundles/jquery")*@
    @RenderSection("scripts", required: false)

    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Organization",
        "url": "http://www.montes.cc",
        "contactPoint": [{
        "@@type": "ContactPoint",
        "telephone": "+505-8-402-4092",
        "contactType": "customer service"
        }]
        }
    </script>
    <script src="~/Scripts/JsMontes.js"></script>
</body>
</html>